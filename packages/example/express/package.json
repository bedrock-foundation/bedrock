{
    "name": "@bedrock-foundation/example-express",
    "version": "0.0.11",
    "description": "Bedrock Express Example",
    "main": "index.js",
    "private": true,
    "directories": {
        "test": "test"
    },
    "scripts": {
        "watch": "npm run build && node --trace-warnings lib/index.js",
        "dev": "npm-watch watch",
        "build": "tsc --project . ",
        "start": "node lib/index.js"
    },
    "watch": {
        "watch": {
            "patterns": [
                "src",
                "node_modules/@bedrock-foundation/server/lib/**/*"
            ],
            "quiet": false,
            "legacyWatch": true,
            "extensions": "ts, js, tsx"
        }
    },
    "author": "sam@bedrock.fyi",
    "license": "ISC",
    "dependencies": {
        "@bedrock-foundation/sdk": "^0.0.11",
        "@bedrock-foundation/server": "^0.0.11",
        "@metaplex-foundation/js-next": "^0.9.0",
        "@solana/pay": "^0.2.0",
        "@solana/spl-token": "^0.2.0",
        "@solana/web3.js": "1.37.0",
        "bignumber.js": "^9.0.2",
        "body-parser": "^1.19.0",
        "bs58": "^5.0.0",
        "chai": "^4.3.4",
        "chai-http": "^4.3.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "fs-extra": "^8.1.0",
        "mocha": "^8.4.0",
        "redis": "^4.1.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.3",
        "@types/fs-extra": "^8.1.2",
        "@types/long": "^3.0.32",
        "@types/mocha": "^2.2.44",
        "@types/node": "^17.0.34",
        "@types/ws": "^8.5.3",
        "npm-watch": "^0.5.0",
        "ts-loader": "^5.4.5",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.2.4"
    }
}
